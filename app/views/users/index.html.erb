<div class="container">
<h1 class="text-center">Users</h1>
<table class="table table-dark table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.name, user %></td>
      <td><%= user.email %></td>
      <td><%= user.phone %></td>
    </tr>
  <% end %>
  </tbody>
</table>
</div>